{
  "expo": {
    "name": "Choma",
    "slug": "choma",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "privacy": "public",
    "platforms": ["ios", "android"],
    "assetBundlePatterns": ["**/*"],
    "description": "Discover and order delicious meals from local chefs with convenient meal planning",
    "keywords": ["food", "delivery", "meals", "recipes", "dining"],
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ],
      "@react-native-firebase/app",
      [
        "@react-native-firebase/auth",
        {
          "autolinking": true
        }
      ],
      [
        "@react-native-firebase/messaging",
        {
          "autolinking": true
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#652815"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.choma.app",
      "buildNumber": "1",
      "googleServicesFile": "./GoogleService-Info.plist",
      "associatedDomains": ["applinks:choma.com"],
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location to show nearby restaurants and estimate delivery times.",
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app uses the camera to upload profile pictures and food photos.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to select profile pictures and food photos."
      }
    },
    "android": {
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.choma.app",
      "versionCode": 4,
      "googleServicesFile": "./android/app/google-services.json",
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"]
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "scheme": "choma",
    "extra": {
      "eas": {
        "projectId": "aaa62329-cbac-4b68-93b1-ec10a2ef1be2"
      }
    },
    "owner": "getchoma"
  }
}
